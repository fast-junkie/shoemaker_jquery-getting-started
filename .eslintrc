{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "globals": {
    "jQuery": true,
    "$": true
  },
  "extends": ["airbnb-base"],
  "rules": {
    "import/extensions": 0,
    "linebreak-style": 0,
    "no-alert": 0,
    "no-bitwise": 0,
    "no-console": 0,
    "no-nested-ternary": 0,
    "no-param-reassign": 0,
    "no-return-assign": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0
  }
}
